<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<BorderPane maxHeight="Infinity" maxWidth="Infinity" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.dkfz.roddy.client.fxuiclient.fxwrappercontrols.ProjectAnalysisTreeItemController" style="-fx-padding: 0;">
    <fx:define>
        <Image url="/imgs/icon_analysis.png" fx:id="projectAnalysis"/>
    </fx:define>
    <stylesheets>
        <URL value="@ProjectTreeItem.css"/>
    </stylesheets>
    <center>
        <GridPane style="-fx-padding: 0;">
            <children>
                <!--<Pane GridPane.columnSpan="1" minWidth="100" prefWidth="100" maxWidth="100" maxHeight="20" GridPane.vgrow="ALWAYS"/>-->
                <Label GridPane.columnSpan="1" fx:id="analyisName" text="Label"  style="-fx-padding: 0;">
                    <graphic>
                        <ImageView>
                            <image>
                                <fx:reference source="projectAnalysis"/>
                            </image>
                        </ImageView>
                    </graphic>
                </Label>
            </children>
        </GridPane>

    </center>

</BorderPane>
