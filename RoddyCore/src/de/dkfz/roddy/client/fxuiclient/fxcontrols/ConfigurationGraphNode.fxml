<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?scenebuilder-stylesheet ConfigurationGraphNode.css?>
<fx:root type="javafx.scene.layout.BorderPane" prefHeight="45.0" prefWidth="150.0" xmlns:fx="http://javafx.com/fxml">
    <center>
        <StackPane fx:id="stackPane">
            <children>
                <GridPane fx:id="mainGrid">
                    <children>
                        <VBox alignment="CENTER" GridPane.halignment="CENTER" GridPane.hgrow="ALWAYS" GridPane.valignment="CENTER" GridPane.vgrow="ALWAYS">
                            <Label fx:id="txtCfgName" alignment="CENTER" style="-fx-font-weight: bold;" text="Some text"/>
                            <Label fx:id="txtCfgType" alignment="CENTER" style="-fx-font-style: italic;&#10;-fx-font-size: 9px;" text="&lt;&lt; Some text &gt;&gt;"/>
                        </VBox>
                        <GridPane fx:id="borderFrame" style="-fx-border-color: darkgrey;"/>
                        <GridPane fx:id="selectionFrame" visible="false" style="-fx-border-color: rgb(182, 250, 250); -fx-border-width: 2;"/>
                    </children>
                </GridPane>
                <GridPane fx:id="grayOverlay" visible="false" style="-fx-background-color: rgba(255, 255, 255, 0.8)"/>
            </children>
        </StackPane>
    </center>
</fx:root>
